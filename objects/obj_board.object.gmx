    if(!canActivate &amp;&amp; alarm[0] = -1) alarm[0] = 8;
    
    if(distance_to_object(obj_player) &lt;= 48 &amp;&amp; 
       distance_to_object(obj_player) &gt;= 17 &amp;&amp; !place_meeting(x, y, obj_player)){
        if(obj_player.Activate &amp;&amp; canActivate &amp;&amp; open &amp;&amp; dirC){
            GiveItem(key_id, 1);
            alarm[1] = 3;
        }
    }
}

if(open = true){
